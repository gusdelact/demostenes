package com.gfi.bin.admctasweb.procesoautomatico.model;

import java.util.List;

/**
 * 
 * @author ESS3ESPP
 *
 */
public class BitacoraCargaAutomaticaOficioModel {
	private Integer idCarga;
	private Integer idEmpresa;
	private String numeroOficio;
	private String tipoOficio;
	private String existeXML;
	private Integer diasPlazo;
	private String numeroFolio;
	
	private String referencia;
	
	/**
	 * Campo para observaciones en caso de error en Oficio
	 */
	private String observaciones;
	
	private List<BitacoraCargaAutomaticaPersonaModel> listaPersonas;
	
	private List<BitacoraCargaAutomaticaDoctoModel> listaDocumentos;
	
	public Integer getIdCarga() {
		return idCarga;
	}
	public void setIdCarga(Integer idCarga) {
		this.idCarga = idCarga;
	}
	public Integer getIdEmpresa() {
		return idEmpresa;
	}
	public void setIdEmpresa(Integer idEmpresa) {
		this.idEmpresa = idEmpresa;
	}
	public String getNumeroOficio() {
		return numeroOficio;
	}
	public void setNumeroOficio(String numeroOficio) {
		this.numeroOficio = numeroOficio;
	}
	public String getTipoOficio() {
		return tipoOficio;
	}
	public void setTipoOficio(String tipoOficio) {
		this.tipoOficio = tipoOficio;
	}
	public String getExisteXML() {
		return existeXML;
	}
	public void setExisteXML(String existeXML) {
		this.existeXML = existeXML;
	}
	public String getObservaciones() {
		return observaciones;
	}
	public void setObservaciones(String observaciones) {
		this.observaciones = observaciones;
	}
	public List<BitacoraCargaAutomaticaPersonaModel> getListaPersonas() {
		return listaPersonas;
	}
	public void setListaPersonas(
			List<BitacoraCargaAutomaticaPersonaModel> listaPersonas) {
		this.listaPersonas = listaPersonas;
	}
	public List<BitacoraCargaAutomaticaDoctoModel> getListaDocumentos() {
		return listaDocumentos;
	}
	public void setListaDocumentos(
			List<BitacoraCargaAutomaticaDoctoModel> listaDocumentos) {
		this.listaDocumentos = listaDocumentos;
	}
	public Integer getDiasPlazo() {
		return diasPlazo;
	}
	public void setDiasPlazo(Integer diasPlazo) {
		this.diasPlazo = diasPlazo;
	}
	public String getNumeroFolio() {
		return numeroFolio;
	}
	public void setNumeroFolio(String numeroFolio) {
		this.numeroFolio = numeroFolio;
	}
	public String getReferencia() {
		return referencia;
	}
	public void setReferencia(String referencia) {
		this.referencia = referencia;
	}
}