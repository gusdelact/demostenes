package com.gfi.bin.admctasweb.comunes.enums;

/**
 * Enumeración de tipos de requerimiento para Oficios
 * @author ESS3VAVC
 *
 */
public enum RequerimientoEnum {

	INFORMACION("INF", "Información"), 
	DOCUMENTACION( "DOC", "Documentación"),
	INF_DOC("INFDOC", "Información y Documentación"),
	INMOVILIZACION("INM", "Inmovilización"),
	INM_DOC("INMDOC", "Inmovilización y Documentación"),
	DESINMOVILIZACION("DES", "Desinmovilización"),
	TRASPASO_FONDOS("TFO", "Traspaso de Fondos");
	
	
	
	
	private RequerimientoEnum(String clave, String descripcion) {
		this.clave=clave;
		this.descripcion=descripcion;
	}
	
	public String getClave() {
		return clave;
	}	

	public String getDescripcion() {
		return descripcion;
	}	

	
	private String descripcion;	
	private String clave;	
	
	
}
