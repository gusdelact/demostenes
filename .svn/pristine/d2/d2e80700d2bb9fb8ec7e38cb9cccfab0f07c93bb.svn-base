CREATE TABLE CNBV_TIP_SOL
(
  TIPO_OFICIO   VARCHAR2(2 BYTE)                NOT NULL,
  ID_TIP_REQ    VARCHAR2(8 BYTE)                NOT NULL,
  ID_TIP_SOL    VARCHAR2(8 BYTE)                NOT NULL,
  DESC_TIP_SOL  VARCHAR2(80 BYTE),
  FH_ALTA       DATE,
  CVE_USU_ALTA  VARCHAR2(8 BYTE),
  FH_ULT_MOD    DATE,
  CVE_USU_MOD   VARCHAR2(8 BYTE)
);

ALTER TABLE CNBV_TIP_SOL ADD (
  CONSTRAINT CNBV_TIP_SOL_PK
 PRIMARY KEY
 (TIPO_OFICIO, ID_TIP_REQ, ID_TIP_SOL));

ALTER TABLE CNBV_TIP_SOL ADD (
  CONSTRAINT CNBV_TIP_SOL_CNBV_TIP_REQ_FK1 
 FOREIGN KEY (TIPO_OFICIO, ID_TIP_REQ) 
 REFERENCES CNBV_TIP_REQ (TIPO_OFICIO,ID_TIP_REQ));
