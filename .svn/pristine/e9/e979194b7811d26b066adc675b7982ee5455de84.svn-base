CREATE TABLE CNBV_BIT_CGA_AUT_PERSONA
(
  ID_CARGA            NUMBER(10)                NOT NULL,
  ID_EMPRESA          NUMBER(8)                 NOT NULL,
  NUM_OFICIO          VARCHAR2(30 BYTE)         NOT NULL,
  TIPO_OFICIO         VARCHAR2(2 BYTE)          NOT NULL,
  NUM_CONSEC_PERSONA  NUMBER(3)                 NOT NULL,
  OBSERVACIONES       VARCHAR2(2000 BYTE)
);


ALTER TABLE CNBV_BIT_CGA_AUT_PERSONA ADD (
  CONSTRAINT CNBV_BIT_CGA_AUT_PERSONA_PK
 PRIMARY KEY
 (ID_CARGA, ID_EMPRESA, NUM_OFICIO, TIPO_OFICIO, NUM_CONSEC_PERSONA));


ALTER TABLE CNBV_BIT_CGA_AUT_PERSONA ADD (
  CONSTRAINT CNBV_BIT_CGA_AUT_PERSONA_FK1 
 FOREIGN KEY (ID_CARGA, ID_EMPRESA, NUM_OFICIO, TIPO_OFICIO) 
 REFERENCES CNBV_BIT_CGA_AUT_OFICIO (ID_CARGA,ID_EMPRESA,NUM_OFICIO,TIPO_OFICIO));
