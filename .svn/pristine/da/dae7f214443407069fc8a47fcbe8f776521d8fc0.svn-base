CREATE TABLE CNBV_ESTATUS_SEG
(
  CVE_ESTATUS    VARCHAR2(8 BYTE)               NOT NULL,
  DESCRIPCION    VARCHAR2(100 BYTE),
  ORDEN_PROCESO  NUMBER(2)
);

COMMENT ON COLUMN CNBV_ESTATUS_SEG.CVE_ESTATUS IS 'Clave del Estatus de Seguimiento';
COMMENT ON COLUMN CNBV_ESTATUS_SEG.DESCRIPCION IS 'Campo de Observaciones de Estatus';

ALTER TABLE CNBV_ESTATUS_SEG ADD (
  CONSTRAINT PK_CNBV_CAT_ESTATUS_SEGUIMIE
 PRIMARY KEY
 (CVE_ESTATUS));
