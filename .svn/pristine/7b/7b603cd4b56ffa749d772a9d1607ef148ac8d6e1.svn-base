package com.gfi.corp.component.genericgridman.dto;

import java.io.Serializable;

/**
 * Clase con los parametros del Grid para armar la consulta a ejecutar.
 * @author LUGL4884
 *
 */
public class GenericGridReq implements Serializable {
	
	/**
	 * UID Serializacion
	 */
	private static final long serialVersionUID = 1L;
	
	private long page;
	private int limit;
	private String filter;
	private String sort;
	private String busquedaGeneral;
	private String sqlBase;
	
	
	/**
	 * @return the page
	 */
	public long getPage() {
		return page;
	}
	/**
	 * @param page the page to set
	 */
	public void setPage(long page) {
		this.page = page;
	}
	/**
	 * @return the limit
	 */
	public int getLimit() {
		return limit;
	}
	/**
	 * @param limit the limit to set
	 */
	public void setLimit(int limit) {
		this.limit = limit;
	}
	/**
	 * @return the filter
	 */
	public String getFilter() {
		return filter;
	}
	/**
	 * @param filter the filter to set
	 */
	public void setFilter(String filter) {
		this.filter = filter;
	}
	/**
	 * @return the sort
	 */
	public String getSort() {
		return sort;
	}
	/**
	 * @param sort the sort to set
	 */
	public void setSort(String sort) {
		this.sort = sort;
	}
	/**
	 * @return the busquedaGeneral
	 */
	public String getBusquedaGeneral() {
		return busquedaGeneral;
	}
	/**
	 * @param busquedaGeneral the busquedaGeneral to set
	 */
	public void setBusquedaGeneral(String busquedaGeneral) {
		this.busquedaGeneral = busquedaGeneral;
	}
	/**
	 * @return the sqlBase
	 */
	public String getSqlBase() {
		return sqlBase;
	}
	/**
	 * @param sqlBase the sqlBase to set
	 */
	public void setSqlBase(String sqlBase) {
		this.sqlBase = sqlBase;
	}

}
