	@Override
	public String toString() {
		StringBuilder value = new StringBuilder();
		
		value.append(String.format("%nnumOficio   :[%s]%n", this.numOficio   ));
		value.append(String.format("tipoOficio  :[%s]%n", this.tipoOficio  ));
		value.append(String.format("numFolio    :[%s]%n", this.numFolio    ));
		value.append(String.format("numExped    :[%s]%n", this.numExped    ));
		value.append(String.format("numRegistro :[%s]%n", this.numRegistro ));
		value.append(String.format("fhRecepcion :[%s]%n", this.fhRecepcion ));
		value.append(String.format("fhEnvio     :[%s]%n", this.fhEnvio     ));
		value.append(String.format("cveAutoridad:[%s]%n", this.cveAutoridad));
		value.append(String.format("nomAutoridad:[%s]%n", this.nomAutoridad));
		value.append(String.format("idEmpresa   :[%d]%n", this.idEmpresa   ));
		value.append(String.format("descEmpresa :[%s]%n", this.descEmpresa ));
		value.append(String.format("cveEmpresa  :[%s]%n", this.cveEmpresa  ));
		value.append(String.format("sitOficio   :[%s]%n", this.sitOficio   ));
		value.append(String.format("tipoCaso    :[%s]%n", this.tipoCaso    ));
		value.append(String.format("tieneAcuse  :[%s]%n", this.tieneAcuse  ));
		value.append(String.format("acuseFlag   :[%s]%n", this.acuseFlag  ));

		return value.toString();
