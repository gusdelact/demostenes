	public void setNumRegistro(String numRegistro) {
		this.numRegistro = numRegistro;
	}
	/**
	 * @return the fhRecepcion
	 */
	@JsonSerialize(using = ExtDateSerializer.class)
	public Date getFhRecepcion() {
		return fhRecepcion;
	}
